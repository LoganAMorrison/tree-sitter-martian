{
  "grammars": [
    {
      "name": "martian",
      "camelcase": "Martian",
      "scope": "source.martian",
      "path": ".",
      "file-types": ["_invocation", "_mrosource", "mro"],
      "highlights": ["queries/highlights.scm"],
      "tags": ["queries/tags.scm"],
      "injection-regex": "^(martian|mro)$"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Tree-sitter parser for the Martian language",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-martian"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
